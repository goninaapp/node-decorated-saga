{
  "name": "@goninaapp/decorated-saga",
  "version": "3.0.0",
  "description": "This package provides a minimal framework to implement the decorated saga pattern based on AWS Kinesis and AWS Lambda.",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goninaapp/node-decorated-saga.git"
  },
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "build": "tsup",
    "test": "jest",
    "test:format": "prettier --check \"src/**/*.ts\""
  },
  "dependencies": {
    "@aws-sdk/client-kinesis": "^3.457.0",
    "@aws-sdk/client-sqs": "^3.454.0",
    "aws-lambda": "^1.0.7",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.129",
    "@types/debug": "^4.1.12",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2"
  },
  "author": "Ferdinand von Hagen",
  "license": "ISC"
}
