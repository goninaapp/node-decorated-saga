{
  "name": "@goninaapp/decorated-saga",
  "version": "2.2.0",
  "description": "This package provides a minimal framework to implement the decorated saga pattern based on AWS Kinesis and AWS Lambda.",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goninaapp/node-decorated-saga.git"
  },
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "build": "tsup",
    "test": "jest",
    "test:format": "prettier --check \"src/**/*.ts\"",
    "test:cdk": "esbuild test/lambda.ts --bundle --minify --sourcemap --platform=node --target=es2020 --outfile=dist.test/index.js && cdk"
  },
  "devDependencies": {
    "@aws-sdk/client-lambda": "^3.441.0",
    "@aws-sdk/credential-providers": "^3.441.0",
    "@nestjs/testing": "^10.2.7",
    "@types/aws-lambda": "^8.10.122",
    "@types/debug": "^4.1.9",
    "@types/jest": "^29.5.5",
    "@types/node": "20.8.7",
    "@types/pg": "^8.10.5",
    "@types/uuid": "^9.0.6",
    "axios": "^1.6.0",
    "constructs": "^10.3.0",
    "esbuild-plugin-inline-import": "^1.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "@aws-sdk/client-kinesis": "^3.438.0",
    "@aws-sdk/client-sqs": "^3.428.0",
    "@aws-sdk/rds-signer": "^3.441.0",
    "@nestjs/common": "^10.2.8",
    "@nestjs/core": "^10.2.8",
    "@nestjs/microservices": "^10.2.8",
    "aws-cdk-lib": "^2.103.1",
    "debug": "^4.3.4",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.1"
  },
  "author": "Ferdinand von Hagen",
  "license": "ISC"
}
