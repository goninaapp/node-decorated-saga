    "test:format": "prettier --check \"src/**/*.ts\""
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.122",
    "@types/jest": "^29.5.5",
    "@types/node": "20.8.3",
    "@types/uuid": "^9.0.4",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "@aws-sdk/client-kinesis": "^3.425.0",
    "aws-lambda": "^1.0.7",
    "uuid": "^9.0.1"
  },
