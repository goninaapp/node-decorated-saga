  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goninaapp/node-decorated-saga.git"
  },
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
